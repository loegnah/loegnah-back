generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Nothing {
  id Int @id @default(autoincrement())

  @@index([id])
}

model BotTelegram {
  id     Int    @id @default(autoincrement())
  name   String @unique
  chatId BigInt @unique

  @@index([name])
  @@index([chatId])
}
